<div class="sq-main-layout">
	<span><%= link "New User", to: bruker_styring_path(@conn, :new) %></span>
	<div class="sq-panel sq-admin-bruker-listepanel sq-black"  style="overflow-y: auto;">
		<table class="sq-tables" width="100%">
			<thead>
		  	<tr>
					<th>Kundenr</th>
  		   	<th>Fornavn</th>
  		   	<th>Etternavn</th>
					<th>Telefon</th>
					<th>Epost</th>
		  	</tr>
			</thead>
		  <tbody>
				<%= for user <- @users do %>
				<tr>
					<td><%= user.id %></td>
					<td><%= user.fname %></td>
					<td><%= user.lname %></td>
					<td><%= user.phone %></td>
					<td><%= user.email %></td>
					<td><%= link "Show", to: bruker_styring_path(@conn, :show, user) %></td>
					<td><button><%= link "Delete", to: bruker_styring_path(@conn, :delete, user), method: :delete, data: [confirm: "Are you sure?"], class: "sq-nounderline sq-text-base", style: "color:red;" %></button></td>
	        <td><button><%= link "Edit", to: bruker_styring_path(@conn, :edit, user), class: "sq-nounderline sq-text-base", style: "color:blue;" %></button></td>
	      </tr>
				</tr>
				<% end %>
		  </tbody>
	  </table>
	</div>


