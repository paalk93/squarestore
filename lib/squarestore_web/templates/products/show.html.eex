<div class="sq-main-layout">
<div class="sq-p-bildepanel sq-panel sq-align-center sq-justify-center">
  <img src="/images/user.png" class="sq-product-img-lg">
</div>
<div class="sq-p-logpanel sq-panel sq-flex-row">
	<div class=" sq-flex-column sq-flex " style="min-width:75%;">
      <h3><%= @product.name %></h3>
			<ul>
        <li>Product number: <%= @product.pnr %></li>
        <li><%= @product.summary %></li>
        <li>In storage: <%= @product.storage_status %></li>
			</ul>
		<a href="http://www.prisjakt.no" class=" sq-margin-2">Sammenlign priser på Prisjakt.no</a>
	</div>
	<div class=" sq-flex-column sq-flex sq-p-align-end" style="min-width:25%;">
		<div class="sq-p-price-card sq-flex sq-justify-center">
			<h3><%= @product.base_price %>-,</h3>
		</div>
		<button type="button" name="button">Handlekorg</button>
		<button type="button" name="button">Kjøp Nå</button>
	</div>


</div>
